<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
            http://www.springframework.org/schema/beans 
            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
            http://www.springframework.org/schema/context 
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config />

    <bean id="testSubscriberA" class="com.rws.pirkolator.core.engine.TestTypeSubscriber">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataA" />
        <property name="name" value="Test Subscriber A" />
    </bean>

    <bean id="testSubscriberB1" class="com.rws.pirkolator.core.engine.TestTypeSubscriber">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataB" />
        <property name="name" value="Test Subscriber B1" />
    </bean>
    
    <bean id="testSubscriberB2" class="com.rws.pirkolator.core.engine.TestTypeSubscriber">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataB" />
        <property name="name" value="Test Subscriber B2" />
    </bean>

    <bean id="testSubscriberC" class="com.rws.pirkolator.core.engine.TestTypeSubscriber">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataC" />
        <property name="name" value="Test Subscriber C" />
    </bean>

    <bean id="testPublisherA" class="com.rws.pirkolator.core.engine.TestTypePublisher">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataA" />
        <property name="name" value="Test Publisher A" />
    </bean>

    <bean id="testPublisherB1" class="com.rws.pirkolator.core.engine.TestTypePublisher">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataB" />
        <property name="name" value="Test Publisher B1" />
    </bean>
    
    <bean id="testPublisherB2" class="com.rws.pirkolator.core.engine.TestTypePublisher">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataB" />
        <property name="name" value="Test Publisher B2" />
    </bean>

    <bean id="testPublisherC" class="com.rws.pirkolator.core.engine.TestTypePublisher">
        <constructor-arg value="com.rws.pirkolator.core.engine.TestDataC" />
        <property name="name" value="Test Publisher C" />
    </bean>
</beans>

